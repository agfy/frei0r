<!DOCTYPE kpartgui>
<effect LC_NUMERIC="C" tag="frei0r.cropNwarp" id="frei0r.cropNwarp">
	<name>cropNwarp</name>
	<description>opencv c0rners2 with crop</description>
	<author>RG</author>

	<parameter type="simplekeyframe" widget="corners" name="0" default="0.333333333333333" min="0" max="6000" factor="6000">
		<name>top left X</name>
	</parameter>
	
        <parameter type="simplekeyframe" widget="corners" name="1" default="0.333333333333333" min="0" max="6000" factor="6000">
		<name>top left Y</name>
	</parameter>
	
        <parameter type="simplekeyframe" widget="corners" name="2" default="0.666666666666667" min="0" max="6000" factor="6000">
		<name>top right X</name>
	</parameter>
	
        <parameter type="simplekeyframe" widget="corners" name="3" default="0.333333333333333" min="0" max="6000" factor="6000">
		<name>top right Y</name>
	</parameter>
	
        <parameter type="simplekeyframe" widget="corners" name="4" default="0.666666666666667" min="0" max="6000" factor="6000">
		<name>bottom right X</name>
	</parameter>
	
        <parameter type="simplekeyframe" widget="corners" name="5" default="0.666666666666667" min="0" max="6000" factor="6000">
		<name>bottom right Y</name>
	</parameter>
	
        <parameter type="simplekeyframe" widget="corners" name="6" default="0.333333333333333" min="0" max="6000" factor="6000">
		<name>bottom left X</name>
	</parameter>
	
        <parameter type="simplekeyframe" widget="corners" name="7" default="0.666666666666667" min="0" max="6000" factor="6000">
		<name>bottom left Y</name>
	</parameter>

        <parameter type="list" name="8" default="0.25" paramlist="0.0;0.25;0.50;0.75;1.0">
                <paramlistdisplay>INTER_NEAREST,INTER_LINEAR,INTER_AREA,INTER_CUBIC,INTER_LANCZOS4</paramlistdisplay>
		<name>Smoothing method</name>
	</parameter>

	<parameter type="constant" name="9" default="0.05" min="0" max="2000" factor="2000">
		<name>Left</name>
	</parameter>
	<parameter type="constant" name="10" default="0.05" min="0" max="2000" factor="2000">
		<name>Top</name>
	</parameter>

	<parameter type="constant" name="11" default="0.95" min="0" max="2000" factor="2000">
		<name>Right</name>
	</parameter>

	<parameter type="constant" name="12" default="0.95" min="0" max="2000" factor="2000">
		<name>Bottom</name>
	</parameter>


        <parameter type="bool" name="13" min="0" max="1" default="0">
                <name>Activate Warp</name>
        </parameter>        

        <parameter type="bool" name="14" min="0" max="1" default="1">
                <name>input is a static PNG</name>
        </parameter>        

        <parameter type="constant" name="15" default="0.0"  min="0" max="100" factor="100">
                <name>edge smoothing strength</name>
        </parameter>        

        <parameter type="constant" name="17" min="0" max="10" default="0">
                <name>Unique Identifier</name>
        </parameter>        

</effect>
